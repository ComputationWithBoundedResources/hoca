
(STRATEGY
    INNERMOST)

(VAR
    x2 x1 x4 x3)
(RULES
    rec[foldr_0][3](step(rev_0()),fleft_0(),Nil()) -> fleft_0()
    rec[foldr_0][3](step(rev_0()),fleft_0(),Cons(x2,x1)) ->
      step_x_f(rev_0(),x2,rec[foldr_0][3](step(rev_0()),fleft_0(),x1))
    step_x_f[1](rev_0(),x4,step_x_f(rev_0(),x3,x2),x1) -> step_x_f[1](rev_0()
                                                                     ,x3
                                                                     ,x2
                                                                     ,Cons(x4
                                                                          ,x1))
    step_x_f[1](rev_0(),x2,fleft_0(),x1) -> Cons(x2,x1)
    main(Nil()) -> Nil()
    main(Cons(x2,x1)) -> step_x_f[1](rev_0()
                                    ,x2
                                    ,rec[foldr_0][3](step(rev_0()),fleft_0(),x1)
                                    ,Nil()))

