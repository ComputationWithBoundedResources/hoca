
(STRATEGY
    INNERMOST)

(VAR
    x2 x1 x4 x3)
(RULES
    foldr_f_z[1](Nil()) -> fleft_0()
    foldr_f_z[2](Nil()) -> Nil()
    foldr_f_z[1](Cons(x2,x1)) -> step_x_f(x2,foldr_f_z[1](x1))
    foldr_f_z[2](Cons(x2,x1)) -> step_x_f[1](x2,foldr_f_z[1](x1),Nil())
    step_x_f[1](x4,step_x_f(x3,x2),x1) -> step_x_f[1](x3,x2,Cons(x4,x1))
    step_x_f[1](x2,fleft_0(),x1) -> Cons(x2,x1)
    main(x1) -> foldr_f_z[2](x1))

