
(STRATEGY
    INNERMOST)

(VAR
    x3 x2 x1)
(RULES
    insert_cond_1(True(),rec[leq_0](),x3,x2,x1) -> Cons(x3,Cons(x2,x1))
    insert_cond_1(False(),rec[leq_0](),x3,x2,x1) ->
      Cons(x2,insert_ord_x[1](rec[leq_0](),x3,x1))
    insert_ord_x[1](rec[leq_0](),x1,Nil()) -> Cons(x1,Nil())
    insert_ord_x[1](rec[leq_0](),x3,Cons(x2,x1)) -> insert_cond_1(leq_x[1](x3
                                                                          ,x2)
                                                                 ,rec[leq_0]()
                                                                 ,x3
                                                                 ,x2
                                                                 ,x1)
    sort_ord[1](rec[leq_0](),Nil()) -> Nil()
    sort_ord[1](rec[leq_0](),Cons(x2,x1)) ->
      insert_ord_x[1](rec[leq_0](),x2,sort_ord[1](rec[leq_0](),x1))
    leq_x[1](0(),x1) -> True()
    leq_x[1](S(x1),0()) -> False()
    leq_x[1](S(x2),S(x1)) -> leq_x[1](x2,x1)
    main(x1) -> sort_ord[1](rec[leq_0](),x1))
(COMMENT
    Types are as follows: 
      0 :: C
      Cons :: [C,D] -> D
      False :: A
      Nil :: D
      S :: [C] -> C
      True :: A
      insert_ord :: [B] -> G
      sort_ord :: [B] -> H
      leq_x :: [I] -> J
      insert_ord_x :: [B,E] -> F
      insert_cond_1 :: [A,B,C,C,D] -> D
      rec[leq_0] :: B
      main :: [D] -> D
      insert[1] :: [B] -> G
      leq[1] :: [I] -> J
      insert_ord[1] :: [B,E] -> F
      sort_ord[1] :: [B,D] -> D
      sort[1] :: [B] -> H
      leq_x[1] :: [C,C] -> A
      insert_ord_x[1] :: [B,C,D] -> D
      rec[insert_0][1] :: [B] -> G
      rec[leq_0][1] :: [I] -> J
      rec[sort_0][1] :: [B] -> H
      insert[2] :: [B,E] -> F
      leq[2] :: [C,C] -> A
      insert_ord[2] :: [B,C,D] -> D
      sort[2] :: [B,D] -> D
      rec[insert_0][2] :: [B,E] -> F
      rec[leq_0][2] :: [C,C] -> A
      rec[sort_0][2] :: [B,D] -> D
      insert[3] :: [B,C,D] -> D
      rec[insert_0][3] :: [B,C,D] -> D 
)
