
(STRATEGY
    INNERMOST)

(VAR
    x2 x1 x4 x3)
(RULES
    take_l_cond(ConsL(x2,x1),0()) -> Nil()
    take_l_cond(ConsL(x4,zipwith_l_f_xs_ys(rec[plus_0](),x3,x2)),S(x1)) ->
      Cons(x4
          ,take_l_cond(zipwith_l_f_xs_ys[1](rec[plus_0](),x3,x2,bot[0]()),x1))
    take_l_cond(ConsL(0(),fibs_1()),S(x1)) ->
      Cons(0()
          ,take_l_cond(ConsL(S(0())
                            ,zipwith_l_f_xs_ys(rec[plus_0]()
                                              ,rec[fibs_0]()
                                              ,fibs_1()))
                      ,x1))
    zipwith_l_cond_1(ConsL(x4,x3),rec[plus_0](),x2,x1) ->
      ConsL(plus_x[1](x2,x4),zipwith_l_f_xs_ys(rec[plus_0](),x1,x3))
    zipwith_l_cond(ConsL(x2,x1),rec[plus_0](),fibs_1()) ->
      zipwith_l_cond_1(ConsL(S(0())
                            ,zipwith_l_f_xs_ys(rec[plus_0]()
                                              ,rec[fibs_0]()
                                              ,fibs_1()))
                      ,rec[plus_0]()
                      ,x2
                      ,x1)
    zipwith_l_cond(ConsL(x4,x3)
                  ,rec[plus_0]()
                  ,zipwith_l_f_xs_ys(rec[plus_0](),x2,x1)) ->
      zipwith_l_cond_1(zipwith_l_f_xs_ys[1](rec[plus_0](),x2,x1,bot[1]())
                      ,rec[plus_0]()
                      ,x4
                      ,x3)
    zipwith_l_f_xs_ys[1](rec[plus_0](),rec[fibs_0](),x2,x1) ->
      zipwith_l_cond(ConsL(0(),fibs_1()),rec[plus_0](),x2)
    zipwith_l_f_xs_ys[1](rec[plus_0]()
                        ,zipwith_l_f_xs_ys(rec[plus_0](),x4,x3)
                        ,x2
                        ,x1) ->
      zipwith_l_cond(zipwith_l_f_xs_ys[1](rec[plus_0](),x4,x3,bot[2]())
                    ,rec[plus_0]()
                    ,x2)
    zipwith_l_f_xs_ys[1](rec[plus_0](),fibs_1(),x2,x1) ->
      zipwith_l_cond(ConsL(S(0())
                          ,zipwith_l_f_xs_ys(rec[plus_0]()
                                            ,rec[fibs_0]()
                                            ,fibs_1()))
                    ,rec[plus_0]()
                    ,x2)
    plus_x[1](0(),x1) -> x1
    plus_x[1](S(x2),x1) -> S(plus_x[1](x2,x1))
    main(x1) -> take_l_cond(ConsL(0(),fibs_1()),x1))

