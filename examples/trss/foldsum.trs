
(STRATEGY
    INNERMOST)

(VAR
    x2 x1 x3 x9)
(RULES
    foldr_f[3](comp(),id(),NilF(),0()) -> 0()
    foldr_f[3](comp(),id(),ConsF(plus_x(x2),x1),0()) ->
      comp_f_g[1](plus_x(x2),rec[foldr_0][3](comp(),id(),x1),0())
    rec[foldr_0][3](comp(),id(),NilF()) -> id()
    rec[foldr_0][3](comp(),id(),ConsF(plus_x(x2),x1)) ->
      comp_f_g(plus_x(x2),rec[foldr_0][3](comp(),id(),x1))
    comp_f_g[1](plus_x(x3),comp_f_g(plus_x(x2),x1),0()) ->
      plus_x[1](x3,comp_f_g[1](plus_x(x2),x1,0()))
    comp_f_g[1](plus_x(x1),id(),0()) -> plus_x[1](x1,0())
    rec[mapF_0][2](rec[plus_0](),Nil()) -> NilF()
    rec[mapF_0][2](rec[plus_0](),Cons(x2,x1)) ->
      ConsF(plus_x(x2),rec[mapF_0][2](rec[plus_0](),x1))
    plus_x[1](0(),x9) -> x9
    plus_x[1](S(x2),x1) -> S(plus_x[1](x2,x1))
    main(x1) -> foldr_f[3](comp(),id(),rec[mapF_0][2](rec[plus_0](),x1),0()))

