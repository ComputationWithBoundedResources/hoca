    fst_cond[1](P(rpm_3(f_0(rec[x_0](f(x3)))),rpm_4(x2)),x1) ->
      Leaf(snd_cond[1](rec[x_0][1](f(x3),bot[10]()),bot[11]()))
    fst_cond[1](P(rpm_8(f_0(rec[x_0](f(x7))),x6,x5)
                 ,rpm_9(f_0(rec[x_0](f(x4))),x3,x2))
               ,x1) -> Node(fst_cond[1](rec[rpm_0][3](x6
                                                     ,f_0(rec[x_0](f(x7)))
                                                     ,bot[2]())
                                       ,bot[3]())
                           ,fst_cond[1](rec[rpm_0][3](x5
                                                     ,f_0(rec[x_0](f(x7)))
                                                     ,bot[4]())
                                       ,bot[5]()))
    rec[min_0][2](0(),x82) -> 0()
    rec[min_0][2](S(x90),0()) -> 0()
    rec[min_0][2](S(x10),S(x6)) -> S(rec[min_0][2](x10,x6))
