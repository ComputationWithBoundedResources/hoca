      P(rpm_8(f_0(rec[x_0](f(x2))),x4,x3),rpm_9(f_0(rec[x_0](f(x2))),x4,x3))
    snd_cond[1](P(rpm_3(f_0(rec[x_0](f(x3)))),rpm_4(x2)),x1) -> x2
    snd_cond[1](P(rpm_8(f_0(rec[x_0](f(x7))),x6,x5)
                 ,rpm_9(f_0(rec[x_0](f(x4))),x3,x2))
               ,x1) ->
      rec[min_0][2](snd_cond[1](rec[rpm_0][3](x3,f_0(rec[x_0](f(x4))),bot[6]())
                               ,bot[7]())
                   ,snd_cond[1](rec[rpm_0][3](x2,f_0(rec[x_0](f(x4))),bot[8]())
                               ,bot[9]()))
    main(x3) -> fst_cond[1](rec[x_0][1](f(x3),bot[12]()),bot[13]()))

