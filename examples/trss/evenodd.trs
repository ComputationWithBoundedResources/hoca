
(STRATEGY
    INNERMOST)

(VAR
    x3)
(RULES
    odd[1](0()) -> False()
    odd[1](S(x3)) -> even[1](x3)
    even[1](0()) -> True()
    even[1](S(x3)) -> odd[1](x3)
    main(x3) -> even[1](x3))
(COMMENT
    Types are as follows: 
      0 :: A
      False :: B
      S :: [A] -> A
      True :: B
      main :: [A] -> B
      even[1] :: [A] -> B
      odd[1] :: [A] -> B 
)
