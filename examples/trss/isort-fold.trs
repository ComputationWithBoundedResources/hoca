
(STRATEGY
    INNERMOST)

(VAR
    x2 x1 x3)
(RULES
    fold_f_z[1](insert_ord(rec[leq_0]()),Nil(),Nil()) -> Nil()
    fold_f_z[1](insert_ord(rec[leq_0]()),Nil(),Cons(x2,x1)) ->
      insert_ord_x[1](rec[leq_0]()
                     ,x2
                     ,fold_f_z[1](insert_ord(rec[leq_0]()),Nil(),x1))
    insert_cond_1(True(),rec[leq_0](),x3,x2,x1) -> Cons(x3,Cons(x2,x1))
    insert_cond_1(False(),rec[leq_0](),x3,x2,x1) ->
      Cons(x2,insert_ord_x[1](rec[leq_0](),x3,x1))
    insert_ord_x[1](rec[leq_0](),x1,Nil()) -> Cons(x1,Nil())
    insert_ord_x[1](rec[leq_0](),x3,Cons(x2,x1)) -> insert_cond_1(leq_x[1](x3
                                                                          ,x2)
                                                                 ,rec[leq_0]()
                                                                 ,x3
                                                                 ,x2
                                                                 ,x1)
    leq_x[1](0(),x1) -> True()
    leq_x[1](S(x1),0()) -> False()
    leq_x[1](S(x2),S(x1)) -> leq_x[1](x2,x1)
    main(x1) -> fold_f_z[1](insert_ord(rec[leq_0]()),Nil(),x1))

