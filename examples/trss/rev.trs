
(STRATEGY
    INNERMOST)

(VAR
    x1 x3 x2 x5)
(RULES
    rec[append_0][2](Nil(),Cons(x1,Nil())) -> Cons(x1,Nil())
    rec[append_0][2](Cons(x3,x2),Cons(x1,Nil())) ->
      Cons(x3,rec[append_0][2](x2,Cons(x1,Nil())))
    rec[rev_0][1](Nil()) -> Nil()
    rec[rev_0][1](Cons(x5,x3)) -> rec[append_0][2](rec[rev_0][1](x3)
                                                  ,Cons(x5,Nil()))
    main(x1) -> rec[rev_0][1](x1))

