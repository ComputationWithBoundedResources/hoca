
(STRATEGY
    INNERMOST)

(VAR
    x1 x3 x2)
(RULES
    append_xs[1](Nil(),Cons(x1,Nil())) -> Cons(x1,Nil())
    append_xs[1](Cons(x3,x2),Cons(x1,Nil())) -> Cons(x3
                                                    ,append_xs[1](x2
                                                                 ,Cons(x1
                                                                      ,Nil())))
    rev[1](Nil()) -> Nil()
    rev[1](Cons(x2,x1)) -> append_xs[1](rev[1](x1),Cons(x2,Nil()))
    main(x1) -> rev[1](x1))

