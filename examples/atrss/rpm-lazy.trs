
(STRATEGY
    INNERMOST)

(VAR
    x2 x3 x4 x7 x5 x6 x10 x11 x9 x8 x12 x0 x1)
(RULES
    @(x(x2),x3) -> @(@(x2,rec[x_0](x2)),bot[0]())
    @(rec[x_0](x2),x3) -> @(x(x2),x3)
    @(fix(),x2) -> rec[x_0](x2)
    @(rpm_3(x4),x7) -> Leaf(@(x4,bot[1]()))
    @(rpm_4(x5),x7) -> x5
    @(rpm_2(x4,x5),x6) -> P(rpm_3(x4),rpm_4(x5))
    rpm_cond(Leaf(x5),x4) -> rpm_2(x4,x5)
    fst_cond(P(x10,x11)) -> x10
    @(fst(),x9) -> fst_cond(x9)
    @(rpm_8(x4,x5,x6),x8) -> Node(@(@(fst()
                                     ,@(@(@(rec[rpm_0](),x5),x4),bot[2]()))
                                   ,bot[3]())
                                 ,@(@(fst()
                                     ,@(@(@(rec[rpm_0](),x6),x4),bot[4]()))
                                   ,bot[5]()))
    min_cond(0(),x10) -> 0()
    min_cond_1(0(),x11) -> 0()
    min_cond_1(S(x12),x11) -> S(@(@(rec[min_0](),x11),x12))
    min_cond(S(x11),x10) -> min_cond_1(x10,x11)
    @(min_a(x9),x10) -> min_cond(x9,x10)
    @(min(),x9) -> min_a(x9)
    @(rec[min_0](),x0) -> @(min(),x0)
    @(rpm_9(x4,x5,x6),x8) -> @(@(rec[min_0]()
                                ,@(@(snd(),@(@(@(rec[rpm_0](),x5),x4),bot[6]()))
                                  ,bot[7]()))
                              ,@(@(snd(),@(@(@(rec[rpm_0](),x6),x4),bot[8]()))
                                ,bot[9]()))
    @(rpm_7(x4,x5,x6),x7) -> P(rpm_8(x4,x5,x6),rpm_9(x4,x5,x6))
    rpm_cond(Node(x5,x6),x4) -> rpm_7(x4,x5,x6)
    @(rpm_t(x3),x4) -> rpm_cond(x3,x4)
    @(rpm(),x3) -> rpm_t(x3)
    @(rec[rpm_0](),x0) -> @(rpm(),x0)
    snd_cond(P(x5,x6)) -> x6
    @(snd(),x4) -> snd_cond(x4)
    @(f_0(x2),x3) -> @(@(snd(),@(x2,bot[10]())),bot[11]())
    @(f(x1),x2) -> @(@(rec[rpm_0](),x1),f_0(x2))
    @(repmin(),x1) -> @(@(fst(),@(@(fix(),f(x1)),bot[12]())),bot[13]())
    main(x0) -> @(repmin(),x0))

