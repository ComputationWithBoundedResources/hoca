
(STRATEGY
    INNERMOST)

(VAR
    x4 x3 x0 x2 x1)
(RULES
    even_cond(0()) -> True()
    odd_cond(0()) -> False()
    odd_cond(S(x4)) -> @(rec[even_0](),x4)
    @(odd(),x3) -> odd_cond(x3)
    @(rec[odd_0](),x0) -> @(odd(),x0)
    even_cond(S(x2)) -> @(rec[odd_0](),x2)
    @(even(),x1) -> even_cond(x1)
    @(rec[even_0](),x0) -> @(even(),x0)
    main(x0) -> @(rec[even_0](),x0))

