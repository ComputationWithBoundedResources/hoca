
(STRATEGY
    INNERMOST)

(VAR
    x6 x7 x5 x0 x3 x4 x2 x1)
(RULES
    sum_cond(Nil()) -> 0()
    plus_cond(0(),x6) -> x6
    plus_cond(S(x7),x6) -> S(@(@(rec[plus_0](),x7),x6))
    @(plus_x(x5),x6) -> plus_cond(x5,x6)
    @(plus(),x5) -> plus_x(x5)
    @(rec[plus_0](),x0) -> @(plus(),x0)
    sum_cond(Cons(x3,x4)) -> @(@(rec[plus_0](),x3),@(rec[sum_0](),x4))
    @(sum(),x2) -> sum_cond(x2)
    @(rec[sum_0](),x0) -> @(sum(),x0)
    map_cond(Nil(),x2) -> Nil()
    map_cond(Cons(x4,x5),x2) -> Cons(@(x2,x4),@(@(rec[map_0](),x2),x5))
    @(map_f(x2),x3) -> map_cond(x3,x2)
    @(map(),x2) -> map_f(x2)
    @(rec[map_0](),x0) -> @(map(),x0)
    mult_cond(0(),x4) -> 0()
    mult_cond(S(x5),x4) -> @(@(rec[plus_0](),x4),@(@(rec[mult_0](),x5),x4))
    @(mult_x(x3),x4) -> mult_cond(x3,x4)
    @(mult(),x3) -> mult_x(x3)
    @(rec[mult_0](),x0) -> @(mult(),x0)
    @(square(),x2) -> @(@(rec[mult_0](),x2),x2)
    enum_cond(0(),x2) -> Nil()
    unfoldr_cond(Nothing(),x4) -> Nil()
    unfoldr_cond(Just(x6),x4) -> Cons(x6,@(@(rec[unfoldr_0](),x4),x6))
    @(unfoldr_f(x4),x5) -> unfoldr_cond(@(x4,x5),x4)
    @(unfoldr(),x4) -> unfoldr_f(x4)
    @(rec[unfoldr_0](),x0) -> @(unfoldr(),x0)
    countdown_cond(0()) -> Nothing()
    countdown_cond(S(x5)) -> Just(x5)
    @(countdown(),x4) -> countdown_cond(x4)
    enum_cond(S(x3),x2) -> Cons(x2,@(@(rec[unfoldr_0](),countdown()),x2))
    @(enum(),x2) -> enum_cond(x2,x2)
    @(sum_sqs(),x1) -> @(rec[sum_0](),@(@(rec[map_0](),square()),@(enum(),x1)))
    main(x0) -> @(sum_sqs(),x0))

