
(STRATEGY
    INNERMOST)

(VAR
    x2 x3 x5 x6 x4 x0 x7 x8 x9 x1)
(RULES
    foldl_cond(Nil(),x2,x3) -> x3
    foldl_cond(Cons(x5,x6),x2,x3) -> @(@(@(rec[foldl_0](),x2),@(@(x2,x3),x5))
                                      ,x6)
    @(foldl_f_z(x2,x3),x4) -> foldl_cond(x4,x2,x3)
    @(foldl_f(x2),x3) -> foldl_f_z(x2,x3)
    @(foldl(),x2) -> foldl_f(x2)
    @(rec[foldl_0](),x0) -> @(foldl(),x0)
    max_cond(0(),x2,x3) -> x3
    max_cond_1(0(),x2,x4) -> x2
    max_cond_1(S(x5),x2,x4) -> S(@(@(rec[max_0](),x4),x5))
    max_cond(S(x4),x2,x3) -> max_cond_1(x3,x2,x4)
    @(max_x(x2),x3) -> max_cond(x2,x2,x3)
    @(max(),x2) -> max_x(x2)
    @(rec[max_0](),x0) -> @(max(),x0)
    scanr_cond(Nil(),x2,x3) -> Cons(x3,Nil())
    scanr_cond_1(Nil(),x2,x5) -> Scanr_error()
    scanr_cond_1(Cons(x7,x8),x2,x5) -> Cons(@(@(x2,x5),x7),Cons(x7,x8))
    scanr_cond(Cons(x5,x6),x2,x3) -> scanr_cond_1(@(@(@(rec[scanr_0](),x2),x3)
                                                   ,x6)
                                                 ,x2
                                                 ,x5)
    @(scanr_f_z(x2,x3),x4) -> scanr_cond(x4,x2,x3)
    @(scanr_f(x2),x3) -> scanr_f_z(x2,x3)
    @(scanr(),x2) -> scanr_f(x2)
    @(rec[scanr_0](),x0) -> @(scanr(),x0)
    f_cond_1(0(),x3) -> 0()
    f_cond_1(M(x4),x3) -> 0()
    f_cond_1(S(x4),x3) -> x3
    f_cond(0(),x2,x3) -> f_cond_1(x3,x3)
    f_cond_2(0(),x3,x4) -> 0()
    f_cond_2(M(x5),x3,x4) -> 0()
    minus_cond(0(),x6,x7) -> 0()
    minus_cond_1(0(),x6,x8) -> x6
    minus_cond_1(S(x9),x6,x8) -> @(@(rec[minus_0](),x8),x9)
    minus_cond(S(x8),x6,x7) -> minus_cond_1(x7,x6,x8)
    @(minus_x(x6),x7) -> minus_cond(x6,x6,x7)
    @(minus(),x6) -> minus_x(x6)
    @(rec[minus_0](),x0) -> @(minus(),x0)
    f_cond_2(S(x5),x3,x4) -> @(@(rec[minus_0](),x3),x4)
    f_cond(M(x4),x2,x3) -> f_cond_2(x3,x3,x4)
    f_cond_3(0(),x2,x3) -> x2
    f_cond_3(M(x5),x2,x3) -> @(@(rec[minus_0](),x2),x5)
    plus_cond(0(),x7) -> x7
    plus_cond(S(x8),x7) -> S(@(@(rec[plus_0](),x8),x7))
    @(plus_x(x6),x7) -> plus_cond(x6,x7)
    @(plus(),x6) -> plus_x(x6)
    @(rec[plus_0](),x0) -> @(plus(),x0)
    f_cond_3(S(x5),x2,x3) -> @(@(rec[plus_0](),x2),x3)
    f_cond(S(x4),x2,x3) -> f_cond_3(x3,x2,x3)
    @(f_x(x2),x3) -> f_cond(x2,x2,x3)
    @(f(),x2) -> f_x(x2)
    @(rec[f_0](),x0) -> @(f(),x0)
    @(mms(),x1) -> @(@(@(rec[foldl_0](),rec[max_0]()),0())
                    ,@(@(@(rec[scanr_0](),rec[f_0]()),0()),x1))
    main(x0) -> @(mms(),x0))

